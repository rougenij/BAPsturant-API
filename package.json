{
  "name": "bapsturant-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development ts-node-dev src/server.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "postinstall": "tsc",
    "test": "NODE_ENV=test jest --detectOpenHandles",
    "reset:db": "npm run rollback:db && npm run migrate:lts:db && npm run seed:db",
    "rollback:db": "knex migrate:rollback --all --knexfile ./src/database/knexfile",
    "seed:db": "knex seed:run --knexfile ./src/database/knexfile",
    "migrate:lts:db": "knex migrate:latest --knexfile ./src/database/knexfile"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.5",
    "@types/supertest": "^2.0.11",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^27.4.5",
    "prettier": "^2.6.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "ts-node-dev": "^1.1.8",
    "tslib": "^2.4.0"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "axios": "^0.26.0",
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "connect-session-knex": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "express-rate-limit": "^6.0.1",
    "express-session": "^1.17.2",
    "helmet": "^4.6.0",
    "http-status": "^1.5.0",
    "joi": "^17.5.0",
    "knex": "^0.95.15",
    "module-alias": "^2.2.2",
    "moment": "^2.29.3",
    "pg": "^8.7.1",
    "typescript": "^4.6.4",
    "winston": "^3.3.3"
  }
}
